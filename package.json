{
  "name": "@hypergraph-xyz/editor",
  "version": "1.0.1",
  "scripts": {
    "start": "run-p start:*",
    "start:server": "nodemon --watch server.js server.js /tmp/hypergraph.txt",
    "start:react": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "prettier-standard && standard",
    "eject": "react-scripts eject",
    "release": "np",
    "version": "npm run build && git add build"
  },
  "bin": {
    "hypergraph-edit": "./bin/hypergraph-edit.js"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "ignore": [
      "build/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "open": "^7.0.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.0",
    "np": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "prettier-standard": "^15.0.1",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0",
    "standard": "^14.3.1",
    "wax-prosemirror-core": "0.0.3",
    "wax-prosemirror-schema": "0.0.3"
  }
}
